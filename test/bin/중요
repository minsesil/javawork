pakage set_map02;
class A{
int a1;
int a2;

A(){
}

B(int b1, int b2){
this.b1=b1;
this.b2=b2;
}
}
class B extends A{
int b1;
int b2;

B(int a1, int a2, b1, int b2){
super(a1,a2);
this.b1=b1;
this b2=b2;
}


}
public class Test{
public static void main(String[] args){
B b = new B(1,2,3,4);
}
}







